{
    "name": "caue-alves/buscador-de-cursos",
    "description": "Busca cursos na plataforma Alura",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Caue Alves",
            "email": "caue.mendes.rodrigues.alves@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "symfony/dom-crawler" : "^5.0",
        "symfony/css-selector" : "^5.0"
    },
    "autoload": {
        "classmap": [
            "./src"
        ],
        "psr-4": {
            "Alura\\Buscador\\" : "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts" : {
        "cs" : "vendor\\bin\\phpcs --standard=PSR12 src\\",
        "ls" : "dir"
    }
}
